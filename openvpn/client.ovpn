client

dev tun
;link-mtu 1400

proto tcp
remote x.x.x.x [port]

resolv-retry infinite

nobind

# Downgrade privileges after initialization (non-Windows only)
;user nobody
;group nobody

# Try to preserve some state across restarts.
persist-key
persist-tun

ns-cert-type server
auth-user-pass

cipher AES-256-CBC

comp-lzo

verb 3

route-method exe
route-delay 2
reneg-sec 0
